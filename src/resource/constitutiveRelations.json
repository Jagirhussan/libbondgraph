        "ports": {
          "0": {
            "effort": {
              "name": "e_",
              "unit": "Volt"
            },
            "flow": {
              "name": "f_",
              "unit": "Ampere"
            },
            "dir": "out"
          },
          "1": {
            "effort": {
              "name": "e_",
              "unit": "Volt"
            },
            "flow": {
              "name": "f_",
              "unit": "Ampere"
            },
            "dir": "in"
          }
        },
        "parameters": {
          "n": {
            "description": "Ratio",
            "dimension": "",
            "value": 1
          }
        },
        "constitutive_relations": [
          "e_1 - n * e_0",
          "f_0 + n * f_1"
        ]
      },
      "Gyrator": {
        "name": "Gyrator",
        "description": "Linear Gyrator",
        "shortname": "GY",
        "variableprefix": "GY",
        "class": "junction",
        "ports": {
          "0": {
            "effort": {
              "name": "e_",
