          },
          "1": {
            "effort": {
              "name": "e_",
              "unit": "Volt"
            },
            "flow": {
              "name": "f_",
              "unit": "Ampere"
            },
            "dir": "out"
          }
        },
        "parameters": {
          "n": {
            "description": "Ratio",
            "dimension": "",
            "value": 1
          }
        },
        "constitutive_relations": [
          "e_1 + n*f_0",
          "e_0 - n*f_1"
        ]
      },
      "OneJunction": {
