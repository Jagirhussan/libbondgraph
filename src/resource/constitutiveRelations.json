{
  "ElementDefinitions": {
    "Electrical": {
      "location": 1,
      "Resistance": {
        "description": "Linear Resistor",
        "name": "Resistor",
        "shortname": "R",
        "variableprefix": "R",
        "class": "passive",
        "parameters": {
          "r": {
            "description": "Resistance",
            "dimension": "Ohm",
            "universalConstant": false,
            "value": 1
          }
        },
        "constitutive_relations": [
          "e_0 - f_0*r"
        ]
      },
      "Capacitance": {
        "description": "Linear Capacitor",
        "name": "Capacitor",
        "shortname": "C",
        "variableprefix": "C",
        "class": "passive",
        "parameters": {
          "C": {
            "description": "Capacitance",
            "dimension": "Farad",
            "value": 1
          }
        },
        "states": {
          "q_0": {
            "description": "Electric charge",
            "dimension": "coulomb",
            "value": 0
          }
        },
        "constitutive_relations": [
          "q_0 - C * e_0",
          "dot_q_0 - f_0"
        ]
      },
      "Inductance": {
        "description": "Linear Inductor",
        "name": "Inductor",
        "shortname": "I",
        "variableprefix": "I",
        "class": "passive",
        "states": {
          "p_0": {
            "description": "Magetic flux",
            "dimension": "weber",
            "value": 0
          }
        },
        "parameters": {
          "L": {
            "description": "Inductance",
            "dimension": "henry",
            "value": 1
          }
        },
        "constitutive_relations": [
          "p_0 - L*f_0",
          "dot_p_0 - e_0"
        ]
      },
      "PotentialSource": {
        "name": "ConstantVoltageSource",
        "description": "Constant Voltage Source",
        "shortname": "Se",
        "variableprefix": "Se",
        "class": "passive",
        "parameters": {
          "u": {
            "description": "Voltage",
            "dimension": "V",
            "value": 1
          }
        },
        "constitutive_relations": [
          "e_0 -u"
        ]
      },
      "FlowSource": {
        "name": "ConstantCurrentSource",
        "description": "Constant Current Source",
        "shortname": "Sf",
        "variableprefix": "Sf",
        "class": "passive",
        "parameters": {
          "i": {
            "description": "Current",
            "dimension": "A",
            "value": 1
          }
        },
        "constitutive_relations": [
          "f_0+i"
        ]
      },
      "BondDimensions": {
        "effort": "V",
        "flow": "A"
      }
    },
    "Biochemical": {
      "location": 2,
      "Reaction": {
        "name": "Reaction",
        "description": "Biochemical Reaction",
        "class": "passive",
        "shortname": "Rx",
        "ports": {
          "0": {
            "dir": "in"
          },
          "1": {
            "dir": "out"
          }
        },
        "parameters": {
          "r": {
            "description": "Reaction Rate"
          },
          "R": {
            "description": "Universal Gas Constant"
          },
          "T": {
            "description": "Temperature"
          }
        },
        "constitutive_relations": [
          "f_0 + f_1",
          "f_0 - r*(exp(e_0/R/T) - exp(e_1/R/T))"
        ]
      },
      "Concentration": {
        "name": "Concentration",
        "description": "Concentration of Chemical Species",
        "class": "passive",
        "shortname": "N",
        "ports": {
          "0": null
        },
        "parameters": {
          "k": {
            "description": "Biochemical Constant; exp(mu_0/RT)/V"
          },
          "R": {
            "description": "Universal Gas Constant"
          },
          "T": {
            "description": "Temperature"
          }
        },
        "states": {
          "a_0": "Molar Quantity"
        },
        "constitutive_relations": [
          "e_0 - R*T*log(k*a_0)",
          "f_0 - da_0"
        ]
      },
      "Stoichiometry": {
        "name": "Stoichiometry",
        "description": "Stoichiometry",
        "class": "junction",
        "shortname": "Y",
        "ports": {
          "0": {
            "dir": "in"
          },
          "1": {
            "dir": "out"
          }
        },
        "parameters": {
          "r0": {
            "description": "Affinity",
            "dimension": "",
            "value": -1
          },
          "r1": {
            "description": "Chemical Power",
            "dimension": "",
            "value": 1
          }
        },
        "constitutive_relations": [
          "r0*e_0 + r1*e_1",
          "f_0/r0 - f_1/r1"
        ]
      },
      "Chemostat": {
        "name": "Chemostat",
        "description": "Constant chemical potential",
        "shortname": "SS",
        "variableprefix": "Ss",
        "class": "passive",
        "ports": {
          "0": "1"
        },
        "parameters": {
          "mu": {
            "description": "Potential",
            "dimension": "J/mol",
            "value": 1
          }
        },
        "constitutive_relations": [
          "e_0 -mu"
        ]
      },
      "Flowstat": {
        "name": "Flowstat",
        "description": "Constant chemical flux",
        "shortname": "Sf",
        "variableprefix": "Sf",
        "class": "passive",
        "ports": {
          "0": "1"
        },
        "parameters": {
          "i": {
            "description": "Chemical Flux",
            "dimension": "mol/s",
            "value": 1
          }
        },
        "constitutive_relations": [
          "f_0+i"
        ]
      }
    },
